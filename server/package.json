{
  "name": "hydra",
  "version": "0.0.0",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist",
    "start:dev": "nodemon --config nodemon.json",
    "test": "nodemon --config nodemon.test.json",
    "clean": "concurrently  \"knex migrate:rollback\"  \"knex migrate:latest\"  \"knex seed:run\""
  },
  "_moduleAliases": {
    "src": "dist/src",
    "controllers": "dist/src/controllers",
    "routes": "dist/src/routes",
    "shared": "dist/src/shared",
    "db": "dist/db"
  },
  "dependencies": {
    "command-line-args": "^5.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "knex": "^0.95.14",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "oracledb": "^5.3.0",
    "pg": "^8.7.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.48",
    "@types/jasmine": "^3.10.2",
    "@types/jsonfile": "^6.0.1",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.10",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "find": "^0.3.0",
    "fs-extra": "^10.0.0",
    "jasmine": "^3.10.0",
    "jsonfile": "^6.1.0",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tslint": "^6.1.1",
    "typescript": "^4.5.2"
  }
}
